@model StarForums.Web.ViewModels.Home.CategoryViewModel

    <div class="text-center">
        <h1>@Model.Name</h1>
        <h3>@Model.Description</h3>

        <table class="table">
            @foreach(var post in Model.Posts)
            {
                <tr>
                    <td>@post.Title</td>
                </tr>
            }
        </table>
    </div>